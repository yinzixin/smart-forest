@model SF.Model.Tree

@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, minimum-scale=1, maximum-scale=1, user-scalable=no">
    <meta name="format-detection" content="telephone=no">
    <title>详情页面</title>
    <link href="/css/treeDetail.css" rel="stylesheet" />
 
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="/js/treeDetail.js" type="text/javascript"></script>
</head>
<body>
    <div class="page-content">
        <div class="box-tab tab1 active">
            <div class="con-head mb-15">
                <img src="@Model.Photo" />
            </div>
            <div class="con-block mb-15">
                <div class="block-line">
                    <span class="line-left">古树编号</span>
                    <span class="line-right">@Model.Number</span>
                </div>
                <div class="block-line">
                    <span class="line-left">古树名称</span>
                    <span class="line-right">@Model.Name</span>
                </div>
                <div class="block-line">
                    <span class="line-left">拉丁名</span>
                    <span class="line-right">@Model.NameLatin</span>
                </div>
            </div>
            <div class="con-block mb-15">
                <div class="block-line">
                    <span class="line-left">古树树龄</span>
                    <span class="line-right">@Model.Age 年</span>
                </div>
                <div class="block-line">
                    <span class="line-left">古树高度</span>
                    <span class="line-right">@Model.Height 米</span>
                </div>
                <div class="block-line">
                    <span class="line-left">胸围</span>
                    <span class="line-right">@Model.ChestSize 厘米</span>
                </div>
                <div class="block-line">
                    <span class="line-left">是否名木</span>
                    <span class="line-right">(@Model.IsFamous?"是":"否")</span>
                </div>
                <div class="block-line">
                    <span class="line-left">是否在册</span>
                    <span class="line-right">是</span>
                </div>
                <div class="block-line">
                    <span class="line-left">生长状况</span>
                    <span class="line-right">很好</span>
                </div>
                <div class="block-line">
                    <span class="line-left">树皮</span>
                    <span class="line-right">很好</span>
                </div>
                <div class="block-line">
                    <span class="line-left">树洞</span>
                    <span class="line-right">很好</span>
                </div>
                <div class="block-line">
                    <span class="line-left">枯枝</span>
                    <span class="line-right">很好</span>
                </div>
                <div class="block-line">
                    <span class="line-left">病虫危害程度</span>
                    <span class="line-right">很好</span>
                </div>
                <div class="block-line">
                    <span class="line-left">冠径南北</span>
                    <span class="line-right">123</span>
                </div>
                <div class="block-line">
                    <span class="line-left">冠径东西</span>
                    <span class="line-right">1212</span>
                </div>
                <div class="block-line">
                    <span class="line-left">树冠完整度</span>
                    <span class="line-right">1231321</span>
                </div>
            </div>
            <div class="con-instruction mb-15">
                <h4 class="ins-title">应用价值</h4>
                <p class="ins-content">
                    农工商用：木材轻软,
                    为制木匣和乐器的良材.种子炒熟可食或榨油,油为不干性油.树皮的纤维洁白,
                    可用以造纸和编绳等.木材刨片可浸出……
                </p>
            </div>
            <div class="con-instruction mb-15">
                <h4 class="ins-title">植物志资料</h4>
                <p class="ins-content">
                    @Model.Information
                </p>
            </div>
        </div>
        <div class="box-tab tab2">
            <div class="con-block mb-15">
                <div class="block-line">
                    <span class="line-left">地理位置</span>
                    <span class="line-right">湖北随州</span>
                </div>
                <div class="block-line">
                    <span class="line-left">经度</span>
                    <span class="line-right">34</span>
                </div>
                <div class="block-line">
                    <span class="line-left">纬度</span>
                    <span class="line-right">28</span>
                </div>
                <div class="block-line">
                    <span class="line-left">地点类别</span>
                    <span class="line-right">28</span>
                </div>
                <div class="block-line">
                    <span class="line-left">城市化程度</span>
                    <span class="line-right">中</span>
                </div>
            </div>
            <div class="con-block mb-15">
                <div class="block-line">
                    <span class="line-left">负责人</span>
                    <span class="line-right">小王</span>
                </div>
                <div class="block-line">
                    <span class="line-left">调查人</span>
                    <span class="line-right">小李</span>
                </div>
                <div class="block-line">
                    <span class="line-left">调查时间</span>
                    <span class="line-right">2017-5-10</span>
                </div>
                <div class="block-line">
                    <span class="line-left">复核人</span>
                    <span class="line-right">小张</span>
                </div>
                <div class="block-line">
                    <span class="line-left">复核时间</span>
                    <span class="line-right">2017-5-10</span>
                </div>
                <div class="block-line">
                    <span class="line-left">上传人</span>
                    <span class="line-right">小红</span>
                </div>
                <div class="block-line">
                    <span class="line-left">长传时间</span>
                    <span class="line-right">2017-5-10</span>
                </div>
            </div>
        </div>
        <div class=" box-tab tab3">
            <div class="con-block mb-15">
                <div class="block-line">
                    <span class="line-left">土质</span>
                    <span class="line-right">中</span>
                </div>
                <div class="block-line">
                    <span class="line-left">排水</span>
                    <span class="line-right">中</span>
                </div>
                <div class="block-line">
                    <span class="line-left">植被</span>
                    <span class="line-right">中</span>
                </div>
                <div class="block-line">
                    <span class="line-left">积水</span>
                    <span class="line-right">无</span>
                </div>
                <div class="block-line">
                    <span class="line-left">雷击</span>
                    <span class="line-right">无</span>
                </div>
                <div class="block-line">
                    <span class="line-left">20M范围内水体情况</span>
                    <span class="line-right">中</span>
                </div>
                <div class="block-line">
                    <span class="line-left">铺装</span>
                    <span class="line-right">无</span>
                </div>
                <div class="block-line">
                    <span class="line-left">地面标高</span>
                    <span class="line-right">无</span>
                </div>
                <div class="block-line">
                    <span class="line-left">古树群</span>
                    <span class="line-right">无</span>
                </div>
                <div class="block-line">
                    <span class="line-left">挡土墙</span>
                    <span class="line-right">无</span>
                </div>
                <div class="block-line">
                    <span class="line-left">支撑</span>
                    <span class="line-right">无</span>
                </div>
                <div class="block-line">
                    <span class="line-left">驳岸</span>
                    <span class="line-right">无</span>
                </div>
                <div class="block-line">
                    <span class="line-left">避雷设施</span>
                    <span class="line-right">无</span>
                </div>
                <div class="block-line">
                    <span class="line-left">保护半径</span>
                    <span class="line-right">无</span>
                </div>
                <div class="block-line">
                    <span class="line-left">根部</span>
                    <span class="line-right">根部状态饱满</span>
                </div>
                <div class="block-line">
                    <span class="line-left">近5年古树及周边变化</span>
                    <span class="line-right">无变化</span>
                </div>
                <div class="block-line">
                    <span class="line-left">最近建筑</span>
                    <span class="line-right">无建筑</span>
                </div>
                <div class="block-line">
                    <span class="line-left">影响树木生长活动</span>
                    <span class="line-right">无影响</span>
                </div>
                <div class="block-line">
                    <span class="line-left">护栏半径</span>
                    <span class="line-right">半径广</span>
                </div>
            </div>
        </div>
        <div class=" box-tab tab4">
            <div class="con-instruction mb-15">
                <h4 class="ins-title">相关故事</h4>
                <p class="ins-content">
                    @Model.Story
                </p>
            </div>
        </div>
        <div style="height: 65px;"></div>
        <ul class="box-footer">
            <li class="item active" data-tab=".tab1">
                <span class="icon icon-tree">古树</span>
            </li>
            <li class="item" data-tab=".tab2">
                <span class="icon icon-loc">位置</span>
            </li>
            <li class="item" data-tab=".tab3">
                <span class=" icon icon-else">其他</span>
            </li>
            <li class="item" data-tab=".tab4">
                <span class="icon icon-book">故事</span>
            </li>
        </ul>
    </div>
</body>
</html>
